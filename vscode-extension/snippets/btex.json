{
    "class": {
        "prefix": [
            "@class",
            "@documentclass"
        ],
        "body": [
            "@class ${0:article}\\"
        ],
        "description": "declare latex's class"
    },
    "document": {
        "prefix": [
            "@document"
        ],
        "body": [
            "@document {",
            "\t$0",
            "}"
        ],
        "description": "Main document"
    },
    "math": {
        "prefix": [
            "@math"
        ],
        "body": [
            "@math {",
            "\t$0",
            "}"
        ],
        "description": "Math"
    },
    "center": {
        "prefix": [
            "@center"
        ],
        "body": [
            "@center {",
            "\t$0",
            "}"
        ],
        "description": "Center"
    },
    "figure": {
        "prefix": [
            "@figure"
        ],
        "body": [
            "@figure (${0:H}) {",
            "\t@graphic ${1:graphic}\\",
            "\t@centering\\",
            "\t@caption ${2:caption}\\",
            "\t@label fig:${1:graphic}\\",
            "}"
        ],
        "description": "Figure"
    },
    "paragraph": {
        "prefix": [
            "@paragraph"
        ],
        "body": [
            "@paragraph ${0:paragraph}\\"
        ],
        "description": "Paragraph"
    },
    "section": {
        "prefix": [
            "@section"
        ],
        "body": [
            "@section (label=sec:${0:section}) ${0:section}\\"
        ],
        "description": "Section"
    },
    "subsection": {
        "prefix": [
            "@subsection"
        ],
        "body": [
            "@subsection (label=subsec:${0:subsection}) ${0:subsection}\\"
        ],
        "description": "Subsection"
    },
    "subsubsection": {
        "prefix": [
            "@subsubsection"
        ],
        "body": [
            "@subsubsection (label=subsubsec:${0:subsubsection}) ${0:subsubsection}\\"
        ],
        "description": "Subsubsection"
    },
    "import": {
        "prefix": [
            "@import",
            "@usepackage"
        ],
        "body": [
            "@import ${0:library}\\"
        ],
        "description": "import a library"
    },
    "use": {
        "prefix": [
            "@use",
            "@graphicspath"
